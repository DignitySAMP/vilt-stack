{
    "name": "dignitysamp/vilt-stack",
    "description": "An out of the box configuration for Vue, Inertia, Laravel and Tailwind. Offers choices between TS or JS and Ziggy.js or Wayfinder.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "DignitySAMP"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/filesystem": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/validation": "^11.0|^12.0",
        "symfony/console": "^7.0"
    },
    "require-dev": {
        "laravel/framework": "^11.0|^12.0",
        "orchestra/testbench-core": "^9.0|^10.0",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\VILT\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\VILT\\ViltServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}